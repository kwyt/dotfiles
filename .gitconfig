[core]
	excludesfile = ~/.gitignore_global
  editor = vim
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[user]
	useConfigOnly = true
	email = kashiyucar@gmail.com
	name = kwyt
[alias]
  s = status --short --branch
  b = branch
  c = checkout
  g = log --graph --date=short --decorate=short --oneline --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
  dn = diff --name-only 
  cm = commit -m
  cp = cherry-pick
  hh = rev-parse --short HEAD
